{
  "name": "grupo4-chotuve-media-server",
  "version": "1.0.0",
  "description": "Chotuve Media Server",
  "author": "Grupo 4",
  "license": "MIT",
  "private": true,
  "main": "app.js",
  "scripts": {
    "pretest": "eslint --ignore-path .gitignore .",
    "fix-lint": "eslint --fix --ignore-path .gitignore .",
    "test": "istanbul cover _mocha -- --recursive",
    "coveralls": "istanbul cover _mocha -- --recursive --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "start": "node app.js | pino-pretty -c -l",
    "dev": "nodemon app.js 0.0.0.0:$PORT | pino-pretty -c -l",
    "debug": "mocha --inspect-brk test/test.js"
  },
  "keywords": [
    "nodejs",
    "bootstrap",
    "express"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "express": "^4.16.4",
    "mongoose": "^5.9.10",
    "mongoose-paginate-v2": "^1.3.9",
    "pino": "^6.2.1",
    "pino-http": "^5.1.0",
    "pino-pretty": "^4.0.0",
    "underscore": "^1.10.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.13",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "istanbul": "^0.4.5",
    "mocha": "^7.1.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "sinon": "^9.0.2"
  }
}
